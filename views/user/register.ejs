 >
  <div class="container py-2 h-100">
    <div class="row d-flex justify-content-center align-items-center h-100">
      <div class="col-xl-8">
        <div class="card rounded-3 text-black">
          <div class="row g-0">
            <div class="col-lg-6">
              <div class="card-body p-md-2 mx-md-2">
                

                <form action="/register" method="post" >
                  <p><b>Please Register</b></p>

                  <div class="form-outline mb-2">
                    <label class="form-label" for="form2Example11"
                      >User Name</label
                    >
                    <span id="names" class="text-danger font-weight-bold"></span>
                    <input
                      type="text"
                      id="name"
                      name="name"
                      class="form-control"
                      placeholder="User Name" 
                    />
                    
                  </div>

                  <div class="form-outline mb-2">
                    <label class="form-label" for="form2Example22">Email</label>
                    <span id="emailid" class="text-danger font-weight-bold"></span>
                    <input
                      type="email"
                      id="email"
                      name="email"
                      class="form-control"
                      placeholder="Email" 
                    />
                    
                  </div>

                  <div class="form-outline mb-2">
                    <label class="form-label" for="form2Example22"
                      >Phone Number</label
                    >
                    <span id="mobileno." class="text-danger font-weight-bold"></span>
                    <input
                      type="number"
                      id="mobile"
                      name="mno"
                      class="form-control"
                      placeholder="Phone Number" 
                    />
                    
                  </div>

                  <div class="form-outline mb-2">
                    <label class="form-label" for="form2Example22"
                      >Password</label
                    >
                    <span id="passwords" class="text-danger font-weight-bold"></span>
                    <input
                      type="password"
                      id="pass"
                      name="password"
                      class="form-control"
                      placeholder="Password" 
                    />
                    
                  </div>

                   <div class="form-outline mb-2">
                    <label class="form-label" for="form2Example22"
                      >Confirm Password</label
                    >
                    <span id="conpassword" class="text-danger font-weight-bold"></span>
                    <input
                      type="password"
                      id="conpass"
                      name="password2"
                      class="form-control"
                      placeholder="Confirm Password" 
                    />
                    
                  </div> 
                  
                    <% if(typeof message !=='undefined' ) { %>
                  <p style="color: rgb(255, 0, 0); font-weight: bold">
                    <%= message %>
                  </p>
                  <% } %>

                  <div class="text-center pt-1 mb-3 pb-1">
                    <button
                      class="btn btn-primary btn-block fa-lg gradient-custom-2 mb-3"
                      type="submit" name="submit" onclick="return validation()"
                    >
                      Register
                    </button>
                  </div>
                  <div
                    class="d-flex align-items-center justify-content-center pb-4"
                  >
                    <button
                      type="button"
                      class="btn btn-primary btn-block fa-lg gradient-custom-2 mb-3"
                    >
                      <a
                        style="text-decoration: none; color: rgb(255, 255, 255)"
                        href="/login"
                        >Login</a
                      >
                    </button>
                  </div>
                </form>
              </div>
            </div>
            <div class="col-lg-6 d-flex gradient-custom-2">
